<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
         version="3.1">
    <enterprise-beans>
        <entity>
            <ejb-name>MMObject</ejb-name>
            <ejb-class>com.netcracker.edu.bestgroup.projects.ssh.entities.MMObject</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.math.BigInteger</prim-key-class>
            <reentrant>true</reentrant>
        </entity>
        <session>
            <ejb-name>MMObjectEJB</ejb-name>
            <ejb-class>com.netcracker.edu.bestgroup.projects.ssh.beans.MMObjectEJB</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
    </enterprise-beans>
</ejb-jar>
